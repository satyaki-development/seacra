  	background-repeat: no-repeat;
  	background-size: contain;
  }
  .buttons {
    background-color: #cc4e61;
    border: none;
    border-radius: 30px;
  }
  .buttons:hover, .buttons:active {
    background-color: #cc4e61;
    border: none;
  }
  .btn-primary:hover,.btn-primary:focus,.btn-primary:active,
  .btn-primary:active:focus:not(:disabled):not(.disabled),
  .btn:focus, .btn:active, .btn:hover, .btn-primary:active:focus, .btn:active:focus{
      box-shadow: none!important;
      outline: 0;
  }
  .buttons:not(:disabled):not(:disabled):active {
    background-color: #ca1b35;
  }
  .controls:not(:disabled):not(:disabled):active {
    border-color: #ca1b35;
  }
</style>
