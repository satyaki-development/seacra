  }

  .introduce:hover{
  	color: black;
  	text-decoration: none;
  }
  .flex-column-1{
  	height: fit-content;
  	width: 100%;

  }
  .flex-column-2{
  	height: fit-content;
  	width: 100%;
  	align-items:center;
    justify-content: center;
  }
  .text-gray{
