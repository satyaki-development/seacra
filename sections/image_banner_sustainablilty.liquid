    /* READ MORE */
    
    .read-more-wrap {
        margin: 0;
        font-size: calc(0.75em + 1.15vw);
    }
    
    .read-more-target {
        opacity: 0;
        max-height: 0;
        font-size: 0;
        transition: 0.25s ease;
    }
    
    .read-more-state:checked~.read-more-target {
        opacity: 1;
        font-size: calc(0.5em + 1.15vw);
        max-height: 999em;
    }
    
    .read-more-state~.read-more-trigger:before {
        content: "View more";
    }
    
    .read-more-state:checked~.read-more-trigger:before {
        content: "View less";
    }
    /* .flickity-page-dot {
Â  display: none;
} */
    
    .read-more-trigger {
