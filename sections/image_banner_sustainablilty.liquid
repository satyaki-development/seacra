        border-radius: 30px 30px 0 0;
    }
    
    .desktop-blur-section {
        position: relative;
        right: 40%;
        padding: 1.5rem 2rem 3rem;
        text-align: center;
    }
    
    @media (max-width: 750px) {
        .slideshow {
            overflow: unset;
        }
        #shopify-section-template--17180309160247__rich-text-0 {
