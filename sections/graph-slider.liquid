{%- style -%}
  #shopify-section-{{ section.id }}{
    --section-padding-top: {{ section.settings.padding_top }}px;
    --section-padding-bottom: {{ section.settings.padding_bottom }}px;
  }
{%- endstyle -%}

<link
  rel="stylesheet"
  href="{{ 'graph-slider.css' | asset_url }}"
  media="print"
  onload="this.media='all'"
>
<link rel="stylesheet" href="{{ 'slick.css' | asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'normalize.min.css' |asset_url }}" media="print" onload="this.media='all'">
<link rel="stylesheet" href="{{ 'slick-theme1.css' | asset_url }}" media="print" onload="this.media='all'">
<noscript>{{ 'graph-slider.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'slick.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'slick-theme1.css' | asset_url | stylesheet_tag }}</noscript>
<div class="gallery gallery-responsive portfolio_slider graph-slider">
<!--   <button type="button" class="slick-prev slick-arrow prev" arial-label="previous" role="button">Previous</button> -->
    <div class="inner slick-slide" data-slick-index="1" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide01" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1224.jpg?v=1669636470"></div>
    <div class="inner slick-slide" data-slick-index="2" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide02" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1217.jpg?v=1669636539"></div>
    <div class="inner slick-slide" data-slick-index="3" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide03" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1218_1.jpg?v=1669636551"></div>
    <div class="inner slick-slide" data-slick-index="4" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide04" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1219_1.jpg?v=1669636589"></div>
    <div class="inner slick-slide" data-slick-index="5" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide05" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1221.jpg?v=1669636614"></div>
    <div class="inner slick-slide" data-slick-index="6" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide06" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1220.jpg?v=1669636639"></div>
    <div class="inner slick-slide" data-slick-index="7" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide07" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1222_1.jpg?v=1669636662"></div>
    <div class="inner slick-slide" data-slick-index="8" aria-hidden="true" tabindex="-1" role="option" aria-describedby="slick-slide08" style="width: 1200px;">
      <img class="graph-image" src="https://cdn.shopify.com/s/files/1/0669/6343/9927/files/Group_1223.jpg?v=1669636683"></div>
  <button type="button" class="slick-next slick-arrow next" aria-label="Next" role="button" style="display: block;position: absolute;z-index: 3;right: 30px;">Next</button>
</div>
<script src="{{ 'slick.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'graph-slider.js' | asset_url }}" defer="defer"></script>
{% schema %}
{
  "name": "graph-slider",
  "tag": "section",
  "class": "section",
  "settings": [],
  "presets": [
  {
    "name": "graph-slider",
      "blocks": []
    }
  ]
}
{% endschema %}
