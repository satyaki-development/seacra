<style>
  .content-sliderBox {
     width: 100%;
     height: auto;
   }
   .sliderBox {
     margin: 0px auto;
     overflow: visible;
     position: relative;
     display: grid;
     grid-template-columns: 1fr auto 1fr;
     align-items: center;
   }
   .sliderBox:before,
   .sliderBox:after {
     content: "";
     display: block;
     border-top: 2px solid #cc4e61;
   }

   .mask {
     overflow: hidden;
     height: 35vh;
     width: 55vw;
     border: 2px solid #cc4e61;
     padding: 5%;
   }

   .sliderBox ul {
     margin: 0;
     padding: 0;
     position: relative;
   }

   .sliderBox li {
     width: auto;
     height: auto;
     position: absolute;
     /* top: -325px; */
     list-style: none;
   }

   .sliderBox .quote {
     font-size: 15px;
     font-style: italic;
     text-align: center;
   }

   .sliderBox .source {
     font-size: 10px;
     text-align: center;
     margin-top: 30px;
     color: #cc4e61;
   }

   .sliderBox li.anim1 {
     -webkit-animation: cycle 15s linear infinite;
     animation: cycle 15s linear infinite;
   }

   .sliderBox li.anim2 {
     -webkit-animation: cycle2 15s linear infinite;
     animation: cycle2 15s linear infinite;
   }

   .sliderBox li.anim3 {
     -webkit-animation: cycle3 15s linear infinite;
     animation: cycle3 15s linear infinite;
   }

   .sliderBox li.anim4 {
     -webkit-animation: cycle4 15s linear infinite;
     animation: cycle4 15s linear infinite;
   }

   .sliderBox li.anim5 {
     -webkit-animation: cycle5 15s linear infinite;
     animation: cycle5 15s linear infinite;
   }

   .sliderBox:hover li {
     -webkit-animation-play-state: paused;
     animation-play-state: paused;
   }

   .pink-it {
     color: #cc4e61;
   }
   .name {
     color: #cc4e61;
   }
