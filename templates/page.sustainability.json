          "settings": {
            "custom_liquid": "<div \"><span style=\"font-size: 26px; font-weight:bold;color: #CC4E61;\">Giving back to the <span style=\"font-size: 26px;font-weight:bold;color:#4a4a4a;\">Environment\n<\/span>\n<\/div>"
          }
